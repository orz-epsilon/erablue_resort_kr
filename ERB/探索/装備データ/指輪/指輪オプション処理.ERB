@オプション効果決定(オプションレベル)
#DIM オプションレベル
#DIM 候補数

VARSET RESULTS
候補数 = ENUMFUNCBEGINSWITH(@"RINGオプション_LV{オプションレベル}_")
LOCAL = RAND:候補数
CALLFORM %RESULTS:LOCAL%

@オプション効果表示文字列作成(ARGS)
#FUNCTIONS
#DIMS オプション効果一時保存, (指輪オプション最大数 * 2)

LOCALS = 
VARSET オプション効果一時保存, ""
SPLIT ARGS, "_", オプション効果一時保存
FOR LOCAL, 0, (指輪オプション最大数 * 2)
	IF オプション効果一時保存:LOCAL == ""
		BREAK
	ENDIF
	LOCALS += @"%オプション効果一時保存:LOCAL%"
	IF STRCOUNT(オプション効果一時保存:LOCAL, "率")
		LOCAL += 1
		IF TOINT(オプション効果一時保存:LOCAL) > 0
			LOCALS += "+"
		ENDIF
		LOCALS += @"%オプション効果一時保存:LOCAL%"
		LOCALS += "% "
	ELSE
		LOCAL += 1
		IF TOINT(オプション効果一時保存:LOCAL) > 0
			LOCALS += "+"
		ENDIF
		LOCALS += @"%オプション効果一時保存:LOCAL% "
	ENDIF
NEXT

RETURNF LOCALS


