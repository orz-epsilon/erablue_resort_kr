						CONTINUE
					SIF BATTLE_STATE:LOCAL:ＨＰ < 1
						CONTINUE
					BATTLE_STATE:LOCAL:ＨＰ = MAX(BATTLE_STATE:LOCAL:ＨＰ - 100, 1)
				NEXT
