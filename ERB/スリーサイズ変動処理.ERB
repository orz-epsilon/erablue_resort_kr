				;太りにくい
				IF 変動数値 > 0
					変動数値 = 変動数値 / 2
				ELSE
					変動数値 = 変動数値 * 3 / 2
				ENDIF
			CASE -2
				;太らない
				SIF 変動数値 > 0
					RETURN 0 
		ENDSELECT
		BASE:対象キャラ:変動ウエスト += 変動数値
	CASEELSE
		BASE:対象キャラ:変動ウエスト += 変動数値
