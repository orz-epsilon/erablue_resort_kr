@バスト変動処理(対象キャラ, 変動理由, 変動数値)
#DIMS 変動理由
#DIM 変動数値
#DIM 対象キャラ

;キャラ個別設定
SELECTCASE 変動理由
	CASE "妊娠"
		IF 変動数値 > 0
			IF CFLAG:対象キャラ:妊娠スリーサイズ成長_Ｂ増加 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:妊娠スリーサイズ成長_Ｂ増加 < 0
				RETURN 0
			ENDIF
		ELSE
			IF CFLAG:対象キャラ:妊娠スリーサイズ成長_Ｂ減少 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:妊娠スリーサイズ成長_Ｂ減少 < 0
				RETURN 0
			ENDIF
		ENDIF
	CASE "愛撫"
		IF 変動数値 > 0
			IF CFLAG:対象キャラ:愛撫スリーサイズ成長_Ｂ増加 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:愛撫スリーサイズ成長_Ｂ増加 < 0
				RETURN 0
			ENDIF
		ELSE
			IF CFLAG:対象キャラ:愛撫スリーサイズ成長_Ｂ減少 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:愛撫スリーサイズ成長_Ｂ減少 < 0
				RETURN 0
			ENDIF
		ENDIF
	CASE "食事"
		IF 変動数値 > 0
			IF CFLAG:対象キャラ:食事スリーサイズ成長_Ｂ増加 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:食事スリーサイズ成長_Ｂ増加 < 0
				RETURN 0
			ENDIF
		ELSE
			IF CFLAG:対象キャラ:食事スリーサイズ成長_Ｂ減少 > 0
				GOTO 条件クリア
			ELSEIF CFLAG:対象キャラ:食事スリーサイズ成長_Ｂ減少 < 0
				RETURN 0
			ENDIF
		ENDIF
ENDSELECT

SIF 変動数値 > 0 && !GETBIT(OPTION変数:愛撫スリーサイズ成長, 0)
	RETURN 0
SIF 変動数値 < 0 && !GETBIT(OPTION変数:愛撫スリーサイズ成長, 1)
	RETURN 0

$条件クリア

RESULT = 0
TRYCALLFORM バスト変動固有処理_{NO:対象キャラ}(対象キャラ, 変動理由, 変動数値)
;RESULTが0なら通常処理へ、非0ならここで返す
SIF RESULT
	RETURN 0

SIF TALENT:対象キャラ:体型固定
	RETURN 0

IF あなた特殊能力:肉体改造術 > 0
	IF 変動数値 > 0
		変動数値 = 変動数値 + 10
	ELSEIF 変動数値 < 0
		変動数値 = 変動数値 - 10
	ENDIF
ENDIF

SELECTCASE 変動理由
	CASE "妊娠"
		BASE:対象キャラ:変動バスト += 変動数値
	CASE "愛撫"
		BASE:対象キャラ:変動バスト += 変動数値
	CASE "食事"
		IF TALENT:対象キャラ:太りやすい == -2
			;太らない
			SIF 変動数値 > 0
				変動数値 = 変動数値 * 2 / 5
		ENDIF
		BASE:対象キャラ:変動バスト += 変動数値
	CASEELSE
		BASE:対象キャラ:変動バスト += 変動数値
ENDSELECT

