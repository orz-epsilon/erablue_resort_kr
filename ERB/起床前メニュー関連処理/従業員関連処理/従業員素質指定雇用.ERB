		IF CSVエディタフラグ
			CALL CHOICE("CSVエディタを終了しますか？（セーブは自動では行われません）", "はい", "いいえ")
			SIF RESULT
				GOTO 従業員_LOOP
		ENDIF
		RETURN 0
	CASE 1010 TO 1020
		INVERTBIT 表示モード:1, (LOCAL % 1000)
ENDSELECT

