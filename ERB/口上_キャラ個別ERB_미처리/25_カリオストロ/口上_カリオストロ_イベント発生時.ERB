	;朝　5時～11時
	CASE 5*60 TO 11*60
		LOCALS = 안녕
	;昼　11時～18時
	CASE 11*60 TO 18*60
		LOCALS = 안녕하세요
	;夜　それ以外
	CASEELSE
		LOCALS = 안녕하세요
ENDSELECT

