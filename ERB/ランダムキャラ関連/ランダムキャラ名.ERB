@ランダムキャラ名前生成(ARG, ARG:1 = 1)
#FUNCTIONS

FOR LOCAL, 0, 200
	SELECTCASE ARG
		CASE 2
			LOCALS '= ランダムキャラ名_エルーン()
		CASE 3
			LOCALS '= ランダムキャラ名_ドラフ()
		CASE 4
			LOCALS '= ランダムキャラ名_ハーヴィン()
		CASEELSE
			LOCALS '= ランダムキャラ名()
	ENDSELECT
	;100回重複した場合または男性の場合、完全ランダム作成に切り替える
	SIF LOCAL > 99 || ARG:1 == 2
		LOCALS '= ランダムキャラ名ジェネレータ(ARG, ARG:1)
	;200回重複した場合さすがに諦める
	SIF LOCAL == 199
		BREAK

	SIF !名前重複チェック(LOCALS)
		BREAK
NEXT
RETURNF LOCALS


@名前重複チェック(ARGS)
#FUNCTION

FOR LOCAL, 0, CHARANUM
	SIF CALLNAME:LOCAL == ARGS
		RETURNF 1
NEXT
RETURNF 0


