
@RAPE_COMNAME_S22_3
#FUNCTIONS
TSTR:コマンド名受渡 = 뿔 잡고 애널 박기

;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_22_3
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ａ系")


@RAPE_COM_TOOLTIP_S22_3
LOCALS = <br>■뿔 잡고 애널 박기<br>
LOCALS += "뒤에서 A 삽입 시, 뿔을 잡고 강하게 몰아붙이는 커맨드。<br>A 후배위, A 스탠딩 백 모드 시에만 실행 가능。"
LOCALS += "<br>RAPE_COM 타입: A계<br>획득 소스: 쾌A・접촉・고통・불결(일정 조건 시)"
LOCALS += "<br>고유 획득 경험: A 성교 경험(TARGET)・C 성교 경험_A 삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S22_3
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

CALL SOURCE_CALC_快Ａ(TARGET, PLAYER, 400)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)

CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 400)

;苦痛
LOCAL = 200 * PAIN_CHECK_A(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)

;貞操観念
IF !TALENT:恋慕
	IF TALENT:貞操 == 1
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 500)
	ELSEIF TALENT:貞操 == 0
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 250)
	ENDIF
ENDIF

CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 200)
RCVAR:TARGET:苦痛コマンドフラグ = 1

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:PLAYER:Ｃ性交経験_Ａ挿入 ++
EXP:Ａ性交経験 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S22_3
;実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("22_3")
	RETURN RESULT
;角必須
SIF !TALENT:TARGET:角あり
	RETURN 0
;体格問題
SIF TALENT:PLAYER:体格 == 体格_ハーヴィン並 && TALENT:TARGET:体格 > 体格_子供
	RETURN 0
SIF TALENT:PLAYER:体格 < TALENT:TARGET:体格 - 2
	RETURN 0
;後ろから系モードじゃない時はアウト
SIF MODE_確認_完全一致("Ａ後背位", PLAYER, TARGET) < 0 && MODE_確認_完全一致("Ａ立ちバック", PLAYER, TARGET) < 0
	RETURN 0
SIF CFLAG:TARGET:睡眠
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S22_3

PRINTFORM %CALLNAME:PLAYER%은(는)
PRINTFORML %CALLNAME:TARGET%의 뿔을 잡고, %TEXTR("핸들처럼 다루며/힘껏 움켜쥐고/부러질 정도의 힘을 주어")%면서 %TEXTR("강하게 허리를 박았다/난폭하게 허리를 흔들었다/욕망대로 엉덩이 구멍을 뚫었다")%



