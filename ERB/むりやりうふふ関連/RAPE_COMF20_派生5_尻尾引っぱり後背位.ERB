
@RAPE_COMNAME_S20_5
#FUNCTIONS
TSTR:コマンド名受渡 = 꼬리 잡아당기기 후배위

;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_20_5
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ｖ系")

@RAPE_COM_TOOLTIP_S20_5
LOCALS = <br>■꼬리 잡아당기기 후배위<br>
LOCALS += "뒤에서 삽입할 때, 꼬리를 잡고 당기면서 강하게 몰아붙이는 커맨드. <br>후배위, 스탠딩 백 모드일 때만 실행 가능."
LOCALS += "<br>COM 타입: V계<br>획득 소스: 정복・쾌V・접촉・고통・불결(일정 조건 시)"
LOCALS += "<br>고유 획득 경험: V성교 경험(TARGET)・C성교 경험_V삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S20_5
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

CALL SOURCE_CALC_征服(PLAYER, TARGET, 150)
CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 400)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)

CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 400)

;苦痛
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)

;貞操観念
IF !TALENT:恋慕
	IF TALENT:貞操 == 1
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 500)
	ELSEIF TALENT:貞操 == 0
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 250)
	ENDIF
ENDIF

CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 200)
RCVAR:TARGET:苦痛コマンドフラグ = 1

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:PLAYER:Ｃ性交経験_Ｖ挿入 ++
EXP:Ｖ性交経験 ++

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S20_5
;実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("20_5")
	RETURN RESULT
;尻尾必須
SIF !TALENT:TARGET:尻尾あり
	RETURN 0
;体格問題
SIF TALENT:PLAYER:体格 == 体格_ハーヴィン並 && TALENT:TARGET:体格 > 体格_子供
	RETURN 0
SIF TALENT:PLAYER:体格 < TALENT:TARGET:体格 - 2
	RETURN 0
;後ろから系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("後背位", PLAYER, TARGET) && !MODE_存在判定_完全一致("立ちバック", PLAYER, TARGET)
	RETURN 0
;指を何かに使ってるなら無理
SIF TEQUIP:PLAYER:指
	RETURN 0
SIF CFLAG:TARGET:睡眠
	RETURN 0
RETURN 1
;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S20_5
#DIM 同時キャラ番号

PRINTFORM %CALLNAME:PLAYER%은(는)
PRINTFORML %CALLNAME:TARGET%의 꼬리를 잡고, 고삐처럼%TEXTR("다루며/당기며")% 강하게 허리를 부딪혔다…

