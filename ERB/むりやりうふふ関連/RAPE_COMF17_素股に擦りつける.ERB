@RAPE_COMTYPE_17
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("うふふ")


@RAPE_COM_TOOLTIP_17
LOCALS = <br>■스마타에 비비다<br>
LOCALS += "상대의 가랑이에 페니스를 끼워 앞뒤로 움직여 기분 좋게 되는 커맨드. <br>※모드화 검토 중. 현재는 단발 커맨드."
LOCALS += "<br>RAPE_COM 타입: 우후후<br>획득 소스: 쾌C・노출・접촉・봉사・불결"
LOCALS += "<br>고유 획득 경험: 없음<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM17
;-------------------------------------------------
;RAPE_COM別FLAGの設定
;-------------------------------------------------


RCVAR:TARGET:汎用喘ぎ_文節低減フラグ = 1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------




CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 150)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 30)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_奉仕(TARGET, PLAYER, 250)

CALL SOURCE_CALC_不潔(TARGET, PLAYER, 150)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 7
	EXP:PLAYER:同性愛経験 += 7
ENDIF

RETURN 1



;-------------------------------------------------
;実行判定
;-------------------------------------------------


@RAPE_COM_ABLE17
;素股実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE(17)
	RETURN RESULT
;ちんこがない場合駄目
SIF TALENT:PLAYER:Ｃ感度 == -2
	RETURN 0
;潤滑が高くないとダメ
SIF PALAM:潤滑 < 2000
	RETURN 0
;調教者がオトコか半人半妖かふたなりじゃないとダメ
SIF !(TALENT:PLAYER:性別 & 2)
	RETURN 0
;バイブ挿入中はダメ
SIF TEQUIP:13
	RETURN 0
;縄使用中は不可
; SIF TEQUIP:縄
; 	RETURN 0

;下半身着衣
;鎧、めくれない服、タイツ、前張りがあると無理
SIF TEQUIP:下腹部前鎧あり
	RETURN 0
SIF TEQUIP:下腹部後鎧あり
	RETURN 0
SIF TEQUIP:下半身服あり && (!TEQUIP:スカートめくり可 && !TEQUIP:ずらし可)
	RETURN 0
SIF TEQUIP:下タイツあり && !TEQUIP:下タイツ破れ
	RETURN 0
SIF TEQUIP:下半身下着あり > 0
	RETURN 0
	
;お風呂場プレイ中はプレイマットが無いとダメ
IF TEQUIP:31
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;膣、ペニスを何かに使ってる・届かないなら無理
SIF TEQUIP:PLAYER:ペニス
	RETURN 0
SIF TEQUIP:TARGET:膣
	RETURN 0
RETURN 1


