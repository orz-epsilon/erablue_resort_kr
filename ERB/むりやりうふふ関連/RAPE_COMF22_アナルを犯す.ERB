TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM22

	$体位_LOOP
	DRAWLINE
	PRINTL ■체위를 선택해 주세요
	PRINTL
	FOR LOCAL, 0, 20
		TRYCCALLFORM RAPE_COM_ABLE22_{LOCAL}
			IF RESULT
				SELECTCASE LOCAL
					CASE 0
						PRINTBUTTONLC "[0]A정상위", 0
					CASE 1
						PRINTBUTTONLC "[1]A후배위", 1
					CASE 2
						PRINTBUTTONLC "[2]A누운 후배위", 2
					CASE 3
						PRINTBUTTONLC "[3]A대면좌위", 3
					CASE 4
						PRINTBUTTONLC "[4]A배면좌위", 4
					CASE 5
						PRINTBUTTONLC "[5]A기승위", 5
					CASE 6
						PRINTBUTTONLC "[6]A측위", 6
					CASE 7
						PRINTBUTTONLC "[7]A 굴곡위", 7
					CASE 8
						PRINTBUTTONLC "[8]A 에키벤", 8
					CASE 9
						PRINTBUTTONLC "[9]A 배면 에키벤", 9
					CASE 10
						PRINTBUTTONLC "[10]A 대면 입위", 10
					CASE 11
						PRINTBUTTONLC "[11]A 스탠딩 백", 11
				ENDSELECT
			ENDIF
		CATCH
			CONTINUE
		ENDCATCH
	NEXT

	PRINTL
	PRINTL
	PRINTBUTTONLC "[999]돌아가기", 999

	$INPUT_LOOP
	INPUT

	LOCAL = RESULT
	SELECTCASE LOCAL
		CASE 0 TO 11
			TRYCALLFORM RAPE_COM_ABLE22_{LOCAL}
			IF RESULT
				IF TARGET == MASTER
					TARGET = PLAYER
					PLAYER = MASTER
					RFLAG:指示モード対象あなたフラグ = 1
					TRYCALLFORM CAN_RAPE_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_RAPE_Ａ体位{LOCAL}(0)
						PLAYER = TARGET
						TARGET = MASTER
						GOTO 体位_LOOP
					ENDIF
					RFLAG:指示モード対象あなたフラグ = 0
					PLAYER = TARGET
					TARGET = MASTER
				ELSE
					TRYCALLFORM CAN_RAPE_Ａ体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_RAPE_Ａ体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
				ENDIF
				TRYCALLFORM RAPE_COM22_{LOCAL}
				;メッセージ用に体位の種類を保存
				RFLAG:コマンド結果受渡し変数 = LOCAL
				RETURN 1
			ENDIF
			GOTO INPUT_LOOP
		CASE 999
			RETURN -1
		CASEELSE
			GOTO INPUT_LOOP
	ENDSELECT


