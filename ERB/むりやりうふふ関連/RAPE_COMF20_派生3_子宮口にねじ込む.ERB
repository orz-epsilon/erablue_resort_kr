
@RAPE_COMNAME_S20_3
#FUNCTIONS
TSTR:コマンド名受渡 = 자궁구에 쑤셔 넣다

;===================================================
;コマンドタイプ
;===================================================
@RAPE_COMTYPE_S_20_3
#FUNCTION
TFLAG:コマンドタイプ受渡 = RAPE_COMTYPE("Ｖ系")


@RAPE_COM_TOOLTIP_S20_3
LOCALS = <br>■자궁구에 쑤셔 넣다<br>
LOCALS += "부드럽게 익은 자궁구에 페니스를 삽입하는 커맨드。<br>V삽입계 모드일 때만 실행 가능。"
LOCALS += "<br>RAPE_COM 타입: V계<br>획득 소스: 쾌V・접촉・고통"
LOCALS += "<br>고유 획득 경험: V성교 경험(TARGET)・포르치오 개발(TARGET)・C성교 경험_V삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@RAPE_COM_S20_3
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
CALLF RAPE_COMNAME_S20_3()
RCVAR:ポルチオ開発フラグ = 5

CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 600)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 200)

CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 500)

;苦痛
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ性交経験 ++
EXP:PLAYER:Ｃ性交経験_Ｖ挿入 ++

;同性愛　レズなら戻り値1ゲイなら戻り値2
IF HETEROSEX(TARGET,PLAYER)
	EXP:同性愛経験 += 4
	EXP:PLAYER:同性愛経験 += 4
ENDIF

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@RAPE_COM_ABLE_S20_3
;子宮口実行判定
;一括管理
SIF GLOBAL_RAPE_COMABLE_S("20_3")
	RETURN RESULT
;妊娠中はダメ
SIF TALENT:TARGET:妊娠
	RETURN 0
;Ｖ挿入系モードじゃない時はアウト
SIF MODE_確認_完全一致("Ｖ挿入系", PLAYER, TARGET) < 0
	RETURN 0
;解放イベ踏んでないとアウト
SIF CFLAG:TARGET:子宮口許可フラグ == 0
	RETURN 0
;オプション封印
SIF OPTION変数:子宮口挿入_封印
	RETURN 0
SIF CFLAG:TARGET:睡眠
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@RAPE_MESSAGE_COM_S20_3

PRINTFORM %CALLNAME:PLAYER%은(는)
PRINTFORML 부드럽게 익은%CALLNAME:TARGET%의 자궁에 페니스를%TEXTR("들이밀었다/쑤셔 넣었다")%…

