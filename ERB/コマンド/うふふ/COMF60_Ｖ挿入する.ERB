	SIF MODE_存在判定_順不同("Ｖ挿入系", PLAYER, TARGET)
		RETURN 0
	SIF MODE_存在判定_プレイヤー側("Ｖ挿入系", PLAYER)
		RETURN 0
	SIF MODE_存在判定_ターゲット側("Ｖ挿入系", TARGET)
		RETURN 0
ENDIF
