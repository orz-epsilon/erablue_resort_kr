;弱点コマンドカテゴリ = V
同時モード許可 = 3
同時モード拡張許可 = 1
オートコマンド許可 = 3
反転オート許可 = 3

@COM_TOOLTIP_60
LOCALS = <br>■V삽입한다<br>
LOCALS += "상대의 질에 삽입하여 섹스를 하는 커맨드.<br>V 삽입계 모드 시, [체위를 바꾼다]로 변화한다.<br>V 삽입계 모드가 되어, 모드를 변경하거나 성행위를 종료할 때까지 지속적으로 쾌락이 발생한다.<br>[처녀]를 잃는다."
LOCALS += "<br>COM 타입: V계<br>획득 소스: 쾌V・노출・접촉・고통・불결(일정 조건 시)"
LOCALS += "<br>고유 획득 경험: V 성교 경험(TARGET)・C 성교 경험_V 삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM60
#DIM ランダム候補, 20
#DIM 強さ初期値記録
#DIM 速さ初期値記録
#DIMS 体位名

IF TFLAG:オートコマンドフラグ
	;オートで実行する場合
	;ランダムで体位を決定して実行する
	VARSET ランダム候補, -1
	LOCAL:1 = 0
	FOR LOCAL, 0, 12
		TRYCALLFORM COM_ABLE60_{LOCAL}
		IF RESULT
			ランダム候補:(LOCAL:1) = LOCAL
			LOCAL:1 ++
		ENDIF
	NEXT
	LOCAL = ランダム候補:(RAND:(LOCAL:1))
	TRYCALLFORM COM60_{LOCAL}
	;メッセージ用に体位の種類を保存
	RFLAG:コマンド結果受渡し変数 = LOCAL
	CALL オート体位変更Ｖ
	RETURN 1
ELSEIF GETBIT(FLAG:モード管理, モードビット_同時)
	;同時挿入
	IF RFLAG:コマンド結果受渡し変数 == 12
		TRYCALLFORM COM60_12(強さ初期値記録, 速さ初期値記録)
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
		$体位_LOOP2
		DRAWLINE
		PRINTL ■속도・강도 초기값을 선택해주세요
		PRINT 　　V 삽입 강도:
		PRINTBUTTON "[-]", 899
		FOR LOCAL, 0, 6
			IF 強さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 900 + LOCAL
			ELSE
				PRINTBUTTON "□", 900 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", 906
		PRINT 　　
		PRINT V 삽입 속도:
		PRINTBUTTON "[-]", 909
		FOR LOCAL, 0, 6
			IF 速さ初期値記録 >= LOCAL
				PRINTBUTTON "■", 910 + LOCAL
			ELSE
				PRINTBUTTON "□", 910 + LOCAL
			ENDIF
		NEXT
		PRINTBUTTON "[+]", 916
		PRINTL
		PRINTL
		IF PLAYER == MASTER
			PRINT ■속도・강도 자동 최적화:
			IF ABL:PLAYER:腰使い < 5
				HTML_PRINT @"<font color='#%カラーパレット_HTML("グレーアウト")%'>[OFF] (腰使いLv5が必要)</font>",1
			ELSEIF あなた挿入最適化フラグ
				HTML_PRINT @"<font color='#%カラーパレット_HTML("黄")%'><button value='920'>[最適化ON]</button></font>",1
			ELSE
				HTML_PRINT @"<button value='920'>[最適化OFF]</button>",1
			ENDIF
			PRINTL
		ENDIF
		PRINTBUTTONLC "[998]결정", 998
		PRINTBUTTONLC "[999]돌아가기", 999

		BINPUT

		LOCAL = RESULT
		SELECTCASE LOCAL
			CASE 900 TO 905
				強さ初期値記録 = LOCAL - 900
				GOTO 体位_LOOP2
			CASE 910 TO 915
				速さ初期値記録 = LOCAL - 910
				GOTO 体位_LOOP2
			CASE 899
				強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 906
				強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 909
				速さ初期値記録 = MAX(速さ初期値記録 - 1, 0)
				GOTO 体位_LOOP2
			CASE 916
				速さ初期値記録 = MIN(速さ初期値記録 + 1, 5)
				GOTO 体位_LOOP2
			CASE 920
				INVERTBIT あなた挿入最適化フラグ, 0
				GOTO 体位_LOOP2
			CASE 999
				RETURN -1
		ENDSELECT
		LOCAL:10 = TARGET
		FOR LOCAL, 0, 同時モード_選択数
			SIF 同時モード_選択キャラ:LOCAL < 1
				BREAK
			RESULT = 0
			TARGET = 同時モード_選択キャラ:LOCAL
			TRYCALLFORM COM_ABLE60_12
			IF RESULT == 0
				TARGET = LOCAL:10
				RETURN -1
			ENDIF
			RESULT = 1
			TRYCALLFORM CAN_体位12(1)
			IF !RESULT
				TRYCALLFORM CAN_体位12(0)
				RETURN -1
			ENDIF
		NEXT
		TARGET = LOCAL:10

		TRYCALLFORM COM60_12(強さ初期値記録, 速さ初期値記録)
		;メッセージ用に体位の種類を保存
		RFLAG:コマンド結果受渡し変数 = 12
	ENDIF
	RETURN 1
ELSE
	IF MODE_存在判定_完全一致("Ｖ挿入系", PLAYER, TARGET)
		強さ初期値記録 = V挿入強さ
		速さ初期値記録 = V挿入速さ
	ELSE
		強さ初期値記録 = 0
		速さ初期値記録 = 0
	ENDIF
	$体位_LOOP
	DRAWLINE
	PRINTL ■체위를 선택해주세요
	PRINTL
	LOCAL:1 = 0
	FOR LOCAL, 0, 20
		TRYCCALLFORM COM_ABLE60_{LOCAL}
			SELECTCASE LOCAL
				CASE 0
					体位名 = 正常位
				CASE 1
					体位名 = 後背位
				CASE 2
					体位名 = 寝バック
				CASE 3
					体位名 = 対面座位
				CASE 4
					体位名 = 背面座位
				CASE 5
					体位名 = 騎乗位
				CASE 6
					体位名 = 側位
				CASE 7
					体位名 = 屈曲位
				CASE 8
					体位名 = 駅弁
				CASE 9
					体位名 = 背面駅弁
				CASE 10
					体位名 = 対面立位
				CASE 11
					体位名 = 立ちバック
				CASEELSE
					CONTINUE
			ENDSELECT
			TRYCALLFORM 固有Ｖ体位ABLE_プレイヤー側%体位名%_{NO:PLAYER}(PLAYER)
			TRYCALLFORM 固有Ｖ体位ABLE_ターゲット側%体位名%_{NO:TARGET}(TARGET)
			IF RESULT
				LOCAL:1 ++
				PRINTBUTTONLC @"[{LOCAL}]%体位名%", LOCAL
			ENDIF
		CATCH
			CONTINUE
		ENDCATCH
	NEXT
	IF !LOCAL:1
		PRINTW 현재 선택 가능한 체위가 없으므로 취소합니다
		RETURN -1
	ENDIF

	PRINTL
	PRINTL
	PRINTL ■속도・강도 초기값을 선택해주세요
	PRINT 　　V 삽입 강도:
	PRINTBUTTON "[-]", 899
	FOR LOCAL, 0, 6
		IF 強さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 900 + LOCAL
		ELSE
			PRINTBUTTON "□", 900 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", 906
	PRINT 　　
	PRINT V 삽입 속도:
	PRINTBUTTON "[-]", 909
	FOR LOCAL, 0, 6
		IF 速さ初期値記録 >= LOCAL
			PRINTBUTTON "■", 910 + LOCAL
		ELSE
			PRINTBUTTON "□", 910 + LOCAL
		ENDIF
	NEXT
	PRINTBUTTON "[+]", 916
	PRINTL
	PRINTL
	IF PLAYER == MASTER
		PRINT ■속도・강도 자동 최적화:
		IF ABL:PLAYER:腰使い < 5
			HTML_PRINT @"<font color='#%カラーパレット_HTML("グレーアウト")%'>[OFF] (腰使いLv5が必要)</font>",1
		ELSEIF あなた挿入最適化フラグ
			HTML_PRINT @"<font color='#%カラーパレット_HTML("黄")%'><button value='920'>[最適化ON]</button></font>",1
		ELSE
			HTML_PRINT @"<button value='920'>[最適化OFF]</button>",1
		ENDIF
		PRINTL
	ENDIF
	PRINTBUTTONLC "[999]돌아가기", 999

	$INPUT_LOOP
	BINPUT

	LOCAL = RESULT
	SELECTCASE LOCAL
		CASE 0 TO 11
			TRYCALLFORM COM_ABLE60_{LOCAL}
			IF RESULT
				IF TARGET == MASTER
					TARGET = PLAYER
					PLAYER = MASTER
					RFLAG:指示モード対象あなたフラグ = 1
					TRYCALLFORM CAN_体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_体位{LOCAL}(0)
						PLAYER = TARGET
						TARGET = MASTER
						GOTO 体位_LOOP
					ENDIF
					RFLAG:指示モード対象あなたフラグ = 0
					PLAYER = TARGET
					TARGET = MASTER
				ELSE
					TRYCALLFORM CAN_体位{LOCAL}(1)
					IF !RESULT
						TRYCALLFORM CAN_体位{LOCAL}(0)
						GOTO 体位_LOOP
					ENDIF
				ENDIF
				TRYCALLFORM COM60_{LOCAL}(強さ初期値記録, 速さ初期値記録)
				;メッセージ用に体位の種類を保存
				RFLAG:コマンド結果受渡し変数 = LOCAL
				
				CALL オート体位変更Ｖ
				RETURN 1
			ENDIF
			GOTO INPUT_LOOP
		CASE 900 TO 905
			強さ初期値記録 = LOCAL - 900
			GOTO 体位_LOOP
		CASE 910 TO 915
			速さ初期値記録 = LOCAL - 910
			GOTO 体位_LOOP
		CASE 899
			強さ初期値記録 = MAX(強さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 906
			強さ初期値記録 = MIN(強さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 909
			速さ初期値記録 = MAX(速さ初期値記録 - 1, 0)
			GOTO 体位_LOOP
		CASE 916
			速さ初期値記録 = MIN(速さ初期値記録 + 1, 5)
			GOTO 体位_LOOP
		CASE 920
			INVERTBIT あなた挿入最適化フラグ, 0
			GOTO 体位_LOOP
		CASE 999
			V挿入強さ = 強さ初期値記録
			V挿入速さ = 速さ初期値記録
			RETURN -1
		CASEELSE
			GOTO INPUT_LOOP
	ENDSELECT
ENDIF

@オート体位変更Ｖ
;Ａ挿入中
IF MODE_存在判定_ターゲット側("Ａ挿入系", TARGET)
	LOCAL = DT_CELL_GET("体位モードデータベース", モードID受渡し, "実行キャラ", 1)
	SELECTCASE RFLAG:コマンド結果受渡し変数
		;正常位
		CASE 0
			CALL MODE_CHANGE("Ａ騎乗位", LOCAL, TARGET)
		;対面座位
		CASE 3
			CALL MODE_CHANGE("Ａ背面座位", LOCAL, TARGET)
		;騎乗位
		CASE 5
			CALL MODE_CHANGE("Ａ後背位", LOCAL, TARGET)
		;駅弁
		CASE 8
			CALL MODE_CHANGE("Ａ立ちバック", LOCAL, TARGET)
		;対面立位
		CASE 10
			CALL MODE_CHANGE("Ａ背面駅弁", LOCAL, TARGET)
	ENDSELECT
ENDIF


