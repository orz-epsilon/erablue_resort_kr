	;陥落のみ
	SIF 陥落チェック(TARGET) == 0
		RETURN 0
	;初ウフフずみ
	SIF 初体験日取得("初うふふ", TARGET, MASTER) < 1
		RETURN 0
	;熟知や経験必要
	SIF EXP:TARGET:肉体熟知 < 500
		RETURN 0
	SIF EXP:PLAYER:Ｃ性交経験_Ｖ挿入 + EXP:TARGET:肉体熟知 < 1500
		RETURN 0
ENDIF

RETURN 1

