;-------------------------------------------------
;秘貝開帳
;愛撫系コマンド、レベル2
;-------------------------------------------------
@COMNAME8
#FUNCTIONS
TSTR:コマンド名受渡 = 보지벌리기
SIF GETBIT(FLAG:モード管理, モードビット_同時)
	TSTR:コマンド名受渡 = 동시에 보지벌리기



;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_8
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ")
弱点コマンドカテゴリ = C_女
同時モード許可 = 999


@COM_TOOLTIP_8
LOCALS = <br>■秘貝開帳<br>
LOCALS += "상대에게 비부를 벌리게 하는 커맨드."
LOCALS += "<br>COM타입: 우후후<br>획득 소스: 쾌C・노출"
LOCALS += "<br>고유 획득 경험: 자위 경험(TARGET・일정 조건 시)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM8
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
;処女、貞操観念
IF TALENT:処女 == 1
	CALL SOURCE_CALC_露出(TARGET, PLAYER, 500)
ELSE
	CALL SOURCE_CALC_露出(TARGET, PLAYER, 300)
ENDIF
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 40)
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;露出癖Lv3以上で自慰経験加算
SIF ABL:露出癖 >= 3
	EXP:自慰経験 ++

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 1
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 1

RETURN 1


@CAN_COM8(ARG)
#DIM CONST 必要実行値 = 18
#DIM 実行値
#DIM ＋表示フラグ

SKIPDISP ARG
PRINTFORML 은밀한 조개 개방(%CALLNAME:TARGET%)
;-------------------------------------------------
;実行できるかの判定
;-------------------------------------------------
実行値 = 0
＋表示フラグ = 0

;すべての命令に共通の要素を考慮
CALL COM_ORDER
実行値 = RESULT
＋表示フラグ = RESULT:1

;ABL:欲望
IF ABL:欲望
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += ABL:欲望 * 3
	PRINTFORM 욕망 LV{ABL:欲望}({ABL:欲望 * 3})
	＋表示フラグ = 1
ENDIF
;ABL:Ｖ感覚
IF ABL:Ｖ感覚
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += ABL:Ｖ感覚 * 2
	PRINTFORM V 감각 LV{ABL:Ｖ感覚}({ABL:Ｖ感覚 * 2})
	＋表示フラグ = 1
ENDIF
;ABL:奉仕精神
IF ABL:奉仕精神
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += ABL:奉仕精神 * 4
	PRINTFORM 봉사 정신 LV{ABL:奉仕精神}({ABL:奉仕精神 * 4})
	＋表示フラグ = 1
ENDIF
;ABL:露出癖
IF ABL:8
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += ABL:露出癖 * 3
	PRINTFORM 노출증 LV{ABL:露出癖}({ABL:露出癖 * 3})
	＋表示フラグ = 1
ENDIF
;ABL:自慰中毒
IF ABL:自慰中毒
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += ABL:自慰中毒 * 3
	PRINTFORM 자위 중독 LV{ABL:自慰中毒}({ABL:自慰中毒 * 3})
	＋表示フラグ = 1
ENDIF

IF MARK:快楽刻印
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += MARK:快楽刻印 * 3
	PRINTFORM 쾌락 각인 LV{MARK:快楽刻印}({MARK:快楽刻印 * 3})
	＋表示フラグ = 1
ENDIF

;PALAM:潤滑（の不足）
IF PALAM:潤滑 < PALAMLV:3
	PRINT  - 
	実行値 -= 5
	PRINT 윤활 부족(5)
	＋表示フラグ = 1
ENDIF
;PALAM:欲情
IF GETPALAMLV(PALAM:欲情, 5)
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += GETPALAMLV(PALAM:欲情, 5) * 3
	PRINTFORM 욕정 LV{GETPALAMLV(PALAM:欲情, 5)}({GETPALAMLV(PALAM:欲情, 5) * 3})
	＋表示フラグ = 1
ENDIF

;恥じらい
IF TALENT:羞恥心 > 0
	PRINT  - 
	実行値 -= 2
	PRINT 수치심(2)
	＋表示フラグ = 1
ENDIF
;快感の否定（淫乱、娼婦で無効）
IF TALENT:快感応答 < 0 && !TALENT:淫乱
	PRINT  - 
	実行値 -= 5
	PRINT 쾌감 부정(5)
	＋表示フラグ = 1
ENDIF
;男嫌い（恋慕、親愛で無効）
IF TALENT:性別嗜好 & 1 && !(TALENT:PLAYER:性別 & 1) && ((!TALENT:恋慕) || PLAYER != MASTER)
	PRINT  - 
	実行値 -= 12
	PRINT 남성 기피(12)
	＋表示フラグ = 1
ENDIF

;処女
IF TALENT:処女 == 1
	PRINT  - 
	実行値 -= 20
	PRINT 처녀(20)
	＋表示フラグ = 1
ELSEIF EXP:Ｖ経験 < EXPLV:2
	PRINT  - 
	実行値 -= 5
	PRINT V 경험 부족(5)
	＋表示フラグ = 1
ENDIF

;調教者がふたなり
IF (TALENT:PLAYER:性別 & 3) == 3
	PRINT  + 
	実行値 += 4
	PRINT 후타나리(4)
	＋表示フラグ = 1
ENDIF

;媚薬
IF TEQUIP:媚薬
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += 6
	PRINT 최음제(6)
	＋表示フラグ = 1
ENDIF

;言いなり
IF TCVAR:言いなり
	SIF ＋表示フラグ
		PRINT  + 
	実行値 += 999
	PRINT 당신의 말대로(999)
	＋表示フラグ = 1
ENDIF

;合計を表示(18以上で実行)
PRINTFORM  = {実行値}

SIF 実行値 < 必要実行値
	PRINT  < 
SIF 実行値 == 必要実行値
	PRINT  = 
SIF 実行値 > 必要実行値
	PRINT  > 
PRINTFORM 실행값{必要実行値}

WAIT
SKIPDISP 0

;実行できない
SIF 実行値 < 必要実行値
	RETURN 0

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE8
;秘貝開帳実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE(8)
	RETURN RESULT
;Vがない場合駄目
SIF TALENT:TARGET:Ｖ感度 == -2
	RETURN 0
SIF GETBIT(TALENT:TARGET:性別, 0) == 0
	RETURN 0
;露出癖が2未満だとダメ
SIF ABL:露出癖< 2
	RETURN 0
;処女の場合、露出癖3以上ないとダメ
SIF TALENT:処女 && ABL:露出癖 < 3
	RETURN 0
;縄使用中は不可
; SIF TEQUIP:縄
; 	RETURN 0

;下半身着衣
;前鎧、めくれない服、タイツ、下着があると無理
SIF TEQUIP:下腹部前鎧あり
	RETURN 0
SIF TEQUIP:下半身服あり && (!TEQUIP:スカートめくり可 && !TEQUIP:ずらし可)
	RETURN 0
SIF TEQUIP:下タイツあり && !TEQUIP:下タイツ破れ
	RETURN 0
SIF TEQUIP:下半身下着あり > 0
	RETURN 0

;オトコだとダメ
SIF !(TALENT:性別 & 1)
	RETURN 0
;お風呂場プレイ中はプレイマットが無いとダメ
IF BATHROOM(CFLAG:PLAYER:現在マップ種別, CFLAG:PLAYER:現在位置)
	SIF ITEM:20 == 0 && NOITEM == 0
		RETURN 0
ENDIF
;指を何かに使ってるなら無理
SIF TEQUIP:TARGET:指
	RETURN 0
;膣を何かに使ってるなら無理
SIF TEQUIP:TARGET:膣
	RETURN 0

RETURN 1

