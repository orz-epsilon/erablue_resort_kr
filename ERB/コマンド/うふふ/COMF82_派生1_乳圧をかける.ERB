@COMTYPE_S_82_1
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ")
;複数人を対象に同時にできるか、出来るなら上限人数を入れる
;同時モード許可 = 

;ランダム弱点の候補にするかを設定。敏感素質を考慮に入れるため、CVABSを入れること
;また特定部位が必要な場合、「_ちんこ」や「_角」などのように追加で表記する
;弱点コマンドカテゴリ = 

;キャラが自動で使用しても良い場合に入れる
オートコマンド許可 = 1
連続オート許可 = 1

@COM_TOOLTIP_S82_1
LOCALS = <br>■유압을 가하다<br>
LOCALS += "가슴을 좌우에서 팔로 끼워, 상대의 페니스를 강하게 압박하는 커맨드."
LOCALS += "<br>COM타입：우후후<br>취득 소스：쾌C・노출・접촉"
LOCALS += "<br>고유 취득 경험：가슴애무 경험(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S82_1
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 150)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 30)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 120)
CALL SOURCE_CALC_奉仕(PLAYER, TARGET, 300)

;奉仕快楽
RCVAR:PLAYER:奉仕コマンドフラグ = 1
RCVAR:PLAYER:汎用喘ぎ_文節低減フラグ = 1
;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;パイズリ経験
EXP:PLAYER:パイズリ経験 += 1

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S82_1
;乳圧をかける実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("82_1")
	RETURN RESULT
;パイズリ系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("パイズリ系", PLAYER, TARGET)
	RETURN 0
;巨乳以上
SIF TALENT:PLAYER:バストサイズ < 1
	RETURN 0
RETURN 1

