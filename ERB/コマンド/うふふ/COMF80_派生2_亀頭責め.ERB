
@COMNAME_S80_2
#FUNCTIONS
TSTR:コマンド名受渡 = 귀두 괴롭히기

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_80_2
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("うふふ")
弱点コマンドカテゴリ = C_ちんこ
オートコマンド許可 = 1

@COM_TOOLTIP_S80_2
LOCALS = <br>■귀두 괴롭힘<br>
LOCALS += "핸드잡 중, 상대의 귀두를 집중적으로 괴롭히는 커맨드."
LOCALS += "<br>COM 타입: 우후후<br>획득 소스: 쾌C・노출・접촉"
LOCALS += "<br>고유 획득 경험: 수음 경험(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S80_2
;奉仕快楽
RCVAR:TARGET:奉仕コマンドフラグ = 1
RCVAR:TARGET:汎用喘ぎ_文節低減フラグ = 1

CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 250)
SIF TALENT:TARGET:器用な指
	CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 50)

CALL SOURCE_CALC_露出(TARGET, PLAYER, 20)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 50)

CALL SOURCE_CALC_接触(PLAYER, TARGET, 50)
CALL SOURCE_CALC_奉仕(PLAYER, TARGET, 250)
CALL SOURCE_CALC_不潔(PLAYER, TARGET, 150)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
;フェラ経験
EXP:PLAYER:手淫経験 += 1

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S80_2
;実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("80_2")
	RETURN RESULT
;手コキ系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("手コキ系", PLAYER, TARGET)
	RETURN 0

;下半身着衣
;前鎧、めくれない服、タイツ、前張りがあると無理
SIF TEQUIP:下腹部前鎧あり
	RETURN 0
SIF TEQUIP:下タイツあり && !TEQUIP:下タイツ破れ
	RETURN 0
SIF TEQUIP:下半身下着あり == 2
	RETURN 0

RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S80_2
PRINTFORML %CALLNAME:PLAYER%은 %CALLNAME:TARGET%의 귀두를 손바닥으로 집중적으로 애무했다…


