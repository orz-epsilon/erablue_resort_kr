
@COMNAME_S60_6
#FUNCTIONS
TSTR:コマンド名受渡 = 목덜미 깨물기 교미

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_60_6
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ｖ系")
弱点コマンドカテゴリ = V
オートコマンド許可 = 1

@COM_TOOLTIP_S60_6
LOCALS = <br>■首筋甘噛み交尾<br>
LOCALS += "목덜미를 물고, 이를 세우면서 허리를 흔드는 커맨드.<br>드라프, 엘룬 및 마조 기질에 효과가 크다."
LOCALS += "<br>체격 차이가 없는 경우의 정상위, 후배위, 대면좌위, 누워서 후배위, 서서 후배위 시에만 실행 가능."
LOCALS += "<br>COM 타입: V계<br>획득 소스: 정복・쾌V・접촉・고통・불결(일정 조건 시)"
LOCALS += "<br>고유 획득 경험: V성교 경험(TARGET)・C성교 경험_V삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S60_6
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------


CALL SOURCE_CALC_征服(PLAYER, TARGET, 150)
CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 400)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)

CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 400)

;苦痛
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)

;貞操観念
IF !TALENT:恋慕
	IF TALENT:貞操 == 1
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 500)
	ELSEIF TALENT:貞操 == 0
		CALL SOURCE_CALC_不潔(TARGET, PLAYER, 250)
	ENDIF
ENDIF

CALL SOURCE_CALC_苦痛(TARGET, PLAYER, 180)
RCVAR:TARGET:苦痛コマンドフラグ = 1
RCVAR:PLAYER:嗜虐コマンドフラグ = 1

;マゾに効果大。ただし苦痛の量が増すのではなくそのほかが増える。
;痛みで締まるので快Ｖも盛っちゃお
IF ABL:TARGET:マゾ性感 > 2
	CALL SOURCE_CALC_征服(PLAYER, TARGET, 80)
	CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 80)
	CALL SOURCE_CALC_快Ｓ(TARGET, PLAYER, 100)
	CALL SOURCE_CALC_歓楽(TARGET, PLAYER, 100)
	EXP:TARGET:苦痛快楽経験 += 1
ELSE
	CALL SOURCE_CALC_反感(TARGET, PLAYER, 60)
ENDIF

;エルーンとドラフにはさらに効果が増す。マゾエルーンが重複でマシマシ効果になるようにこの調整。
;猫は痛みで排卵するって言うし、噛まれて無意識に腰へこへこしちゃうマゾユエルは捗るのでこれでヨシ！
IF TALENT:TARGET:種族 == 2 || TALENT:TARGET:種族 == 3
	CALL SOURCE_CALC_征服(PLAYER, TARGET, 80)
	CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 80)
	CALL SOURCE_CALC_快Ｓ(TARGET, PLAYER, 100)
	CALL SOURCE_CALC_歓楽(TARGET, PLAYER, 120)
	EXP:TARGET:苦痛快楽経験 += 1
ENDIF

;サドは嗜虐快楽
SIF ABL:PLAYER:サド性感
	EXP:PLAYER:嗜虐快楽経験 ++


;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:PLAYER:Ｃ性交経験_Ｖ挿入 ++
EXP:Ｖ性交経験 ++

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S60_6
;実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("60_6")
	RETURN RESULT
;噛む側が口を何かに使ってるなら無理
SIF TEQUIP:PLAYER:口
	RETURN 0

;体格問題 TARGETのほうが3サイズ大きいと届かない
SIF 体格差計算(PLAYER, TARGET) >= 3
	RETURN 0
;組み敷く系や密着系モードじゃない時はアウト。背面座位とかでもできないことはないんだけど……シチュ的にどうだろ？
SIF !MODE_存在判定_完全一致("正常位", PLAYER, TARGET) && !MODE_存在判定_完全一致("対面座位", PLAYER, TARGET) && !MODE_存在判定_完全一致("後背位", PLAYER, TARGET) && !MODE_存在判定_完全一致("立ちバック", PLAYER, TARGET) && !MODE_存在判定_完全一致("寝バック", PLAYER, TARGET)
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S60_6
#DIM 同時キャラ番号
PRINTFORM %CALLNAME:PLAYER%는
PRINTFORML %CALLNAME:TARGET%의 목덜미에 %TEXTR("물어뜯으며/이를 박으며")% %TEXTR("강하게/격렬하게")% 허리를 박아 넣었다

IF ABL:TARGET:マゾ性感 > 2 || TALENT:TARGET:種族 == 2 || TALENT:TARGET:種族 == 3 || TALENT:種族 == 5 || 小等部魔法少女判定(TARGET) || 首噛み弱点判定(TARGET)
	PRINTFORM 고통에 교성을 지르는 %CALLNAME:TARGET%의 몸은 %TEXTR("피지배의 마조 성감에 전율하며,/누가 '주인님'인지를 고분고분하게 이해하고,/이 상황에서의 상하 관계를 본능적으로 이해하고,")%
	PRINTFORML %TEXTR("질육을 강하게 조여/반사적으로 허리를 비벼 대듯이/무의식중에 엉덩이를 흔들며/페니스를 깊숙이 머금고, 자궁을 바치며")% 아양떨고 있다
ENDIF
