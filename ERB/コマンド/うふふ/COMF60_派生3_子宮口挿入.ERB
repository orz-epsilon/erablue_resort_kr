
@COMNAME_S60_3
#FUNCTIONS
TSTR:コマンド名受渡 = 자궁구 삽입

;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_60_3
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ｖ系")
SIF CFLAG:TARGET:睡眠
	TFLAG:コマンドタイプ受渡 = COMTYPE("セクハラ強")
睡眠中許可 = 1
弱点コマンドカテゴリ = V
オートコマンド許可 = 1

@COM_TOOLTIP_S60_3
LOCALS = <br>■자궁구 삽입<br>
LOCALS += "부드럽고 성숙한 자궁구에 페니스를 삽입하는 커맨드. <br>V 삽입계 모드 시에만 실행 가능."
LOCALS += "<br>COM 타입: V계<br>획득 소스: 쾌V・접촉・고통"
LOCALS += "<br>고유의 획득 경험: V 성교 경험(TARGET)・포르치오 개발(TARGET)・C 성교 경험_V 삽입(PLAYER)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S60_3
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
CALLF COMNAME_S60_3()
RCVAR:ポルチオ開発フラグ = 5

CALL SOURCE_CALC_快Ｖ(TARGET, PLAYER, 600)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 200)

CALL SOURCE_CALC_快Ｃ(PLAYER, TARGET, 500)

;苦痛
LOCAL = 200 * PAIN_CHECK_V(TARGET) / 1000
CALL SOURCE_CALC_苦痛(TARGET, PLAYER, LOCAL)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｖ性交経験 ++
EXP:子宮姦経験 ++
EXP:PLAYER:Ｃ性交経験_Ｖ挿入 ++

;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S60_3
;子宮口実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("60_3")
	RETURN RESULT
;妊娠中はダメ
SIF TALENT:TARGET:妊娠
	RETURN 0
;Ｖ挿入系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("Ｖ挿入系", PLAYER, TARGET)
	RETURN 0
;解放イベ踏んでないとアウト
SIF CFLAG:TARGET:子宮口許可フラグ == 0
	RETURN 0
;オプション封印
SIF OPTION変数:子宮口挿入_封印
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S60_3
#DIM 同時キャラ番号
PRINTFORM %CALLNAME:PLAYER%는
PRINTFORML 부드럽게 농익은 %CALLNAME:TARGET%의 자궁에 페니스를 쑤셔 넣었다…

