;===================================================
;コマンドタイプ
;===================================================
@COMTYPE_S_62_10
#FUNCTION
TFLAG:コマンドタイプ受渡 = COMTYPE("Ａ系_被挿入中")
弱点コマンドカテゴリ = C_ちんこ
オートコマンド許可 = 1
連続オート許可 = 1

@COM_TOOLTIP_S62_10
LOCALS = <br>■장내를 조이다<br>
LOCALS += "괄약근을 조여 삽입된 음경에 강한 자극을 주는 커맨드."
LOCALS += "<br>COM타입：A계or피삽입중<br>취득 소스：쾌C・노출・접촉"
LOCALS += "<br>고유의 취득 경험：C성교경험_A삽입(TARGET)<br>"
TSTR:ツールチップ受渡 = %LOCALS%

@COM_S62_10
;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------

CALL SOURCE_CALC_快Ａ(PLAYER, TARGET, 600)
CALL SOURCE_CALC_露出(TARGET, PLAYER, 50)
CALL SOURCE_CALC_接触(TARGET, PLAYER, 150)
CALL SOURCE_CALC_快Ｃ(TARGET, PLAYER, 600)

;-------------------------------------------------
;経験の処理
;-------------------------------------------------
EXP:Ｃ性交経験_Ａ挿入 ++
EXP:PLAYER:Ａ性交経験 ++
;同性愛
SIF 同性愛性癖判定(TARGET,PLAYER)
	EXP:TARGET:同性愛経験 += 4
SIF 同性愛性癖判定(PLAYER,TARGET)
	EXP:PLAYER:同性愛経験 += 4

EXP:愛情経験 += TALENT:恋慕 * 2

RETURN 1

;-------------------------------------------------
;実行判定
;-------------------------------------------------
@COM_ABLE_S62_10
;腸内を締める実行判定
SIF SAVESTR:ゲームフェイズ管理 != "通常モード"
	RETURN 0
;一括管理
SIF GLOBAL_COMABLE_S("62_10")
	RETURN RESULT
;挿入系モードじゃない時はアウト
SIF !MODE_存在判定_完全一致("Ａ挿入系", TARGET, PLAYER)
	RETURN 0
RETURN 1

;-------------------------------------------------
;実行時メッセージ
;-------------------------------------------------
@MESSAGE_COM_S62_10
