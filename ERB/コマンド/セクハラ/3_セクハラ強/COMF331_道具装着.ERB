		ENDIF
	CASE 5
		;アナルビーズ
		;PALAM:潤滑（の不足）
		IF PALAM:潤滑 < PALAMLV:3
			PRINT  - 
			実行値 -= 5
			PRINT 윤활 부족(5)
			＋表示フラグ = 1
		ENDIF
		;ABL:A感覚
		IF ABL:Ａ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ａ感覚 * 2
			PRINTFORM A 감각LV{ABL:Ａ感覚}({ABL:Ａ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;A経験
		IF EXP:Ａ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT A 경험 부족(15)
			＋表示フラグ = 1
		ENDIF
	CASE 6
		;ニプルキャップ
		;ABL:Ｂ感覚
		IF ABL:Ｂ感覚
			SIF ＋表示フラグ
				PRINT  + 
			実行値 += ABL:Ｂ感覚 * 2
			PRINTFORM B 감각LV{ABL:Ｂ感覚}({ABL:Ｂ感覚 * 2})
			＋表示フラグ = 1
		ENDIF
		;B経験
		IF EXP:Ｂ経験 < EXPLV:2
			PRINT  - 
			実行値 -= 15
			PRINT B 경험 부족(15)
			＋表示フラグ = 1
		ENDIF
ENDSELECT

