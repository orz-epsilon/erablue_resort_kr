				CALL SOURCE_CALC_露出(TARGET, PLAYER, 150)
				CALL SOURCE_CALC_好感度要素_性愛度UP(TARGET, PLAYER, 120)
		ENDSELECT
	CASE 7
		;愉悦
		SELECTCASE RFLAG:コマンド結果受渡し変数
			CASE 1
				;拒絶
				CALL SOURCE_CALC_歓楽(TARGET, PLAYER, 50)
				CALL SOURCE_CALC_接触(TARGET, PLAYER, 100)
