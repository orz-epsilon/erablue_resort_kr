	CALL SOURCE_CALC_快Ａ(PLAYER, TARGET, 400)
	;A処女喪失
	CALL LOST_A_VIRGIN(PLAYER, TARGET, 1)
	CALL LOST_MAN_A_VIRGIN(TARGET, PLAYER)
