ENDIF
IF TALENT:PLAYER:Ａ感度 != -2
	IF TEQUIP:PLAYER:アナル == 0
		LOCALS = A 삽입
		SIF TALENT:PLAYER:Ａ処女
			LOCALS += "(A 처녀)"
		PRINTBUTTONLC @"[2] %LOCALS%", 2
		LOCAL += 1
	ENDIF
ENDIF
SIF LOCAL == 0
	PRINT 삽입할 곳이 없습니다.
PRINTL
PRINTL
PRINTBUTTONLC "[999]돌아가기", 999

BINPUT

LOCAL = RESULT
SELECTCASE LOCAL
	CASE 1 TO 2
		TRYCALLFORM CAN_逆オナホ(1, LOCAL)
		IF !RESULT
			TRYCALLFORM CAN_逆オナホ(0, LOCAL)
			RESTART
		ENDIF
	CASE 999
		RETURN -1
ENDSELECT

RFLAG:コマンド結果受渡し変数 = LOCAL

;-------------------------------------------------
;ソース追加処理
;-------------------------------------------------
IF LOCAL == 1
	CALL MODE_CHANGE("ハーヴィンオナホＶ", TARGET, PLAYER)
