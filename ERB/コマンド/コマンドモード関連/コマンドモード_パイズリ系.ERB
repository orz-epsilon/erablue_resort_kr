@MODETYPE_パイズリ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%의%TEXTR("페니스/자지/육봉")%을(를)
		IF TALENT:ターゲット番号:バストサイズ == 3
		;魔乳 デカパイも入れたいけどしっかり魔乳に区分したほうが嬉しいかもしれないので魔乳出やすいようにふたつ入れとく。以下も同様
			詳細文文字列受け渡し変数 += @"묵직하게 무거운%TEXTR("마유/마유/왕가슴")%으로 끼워, %TEXTR("위아래로 움직이게 하다/가슴살로 주무르다/가슴 압력으로 주무르다")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 2
		;爆乳
			詳細文文字列受け渡し変数 += @"탐스럽게 영근%TEXTR("폭유/폭유/왕가슴")%으로 끼워, %TEXTR("위아래로 움직이게 하다/가슴살로 주무르다/가슴 압력으로 주무르다")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 1
		;巨乳
			詳細文文字列受け渡し変数 += @"풍만하게 자란 거유로 끼워, %TEXTR("위아래로 움직이게 하다/가슴살로 주무르다/가슴 압력으로 주무르다")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 0
		;普乳
			詳細文文字列受け渡し変数 += @"부드러운%TEXTR("가슴/미유")%으로 끼워, %TEXTR("위아래로 움직이게 하다/가슴살로 주무르다/가슴 압력으로 주무르다")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -1
		;貧乳
			詳細文文字列受け渡し変数 += @"톡 튀어나와 탄력 있는%TEXTR("작은 가슴/미유")%으로 %TEXTR("애무/자극")%한다"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -2
		;絶壁
			詳細文文字列受け渡し変数 += @"자그마한 미유로%TEXTR("애무/자극")%한다"
		ENDIF
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%의
	IF TALENT:ターゲット番号:バストサイズ <= -1
	;貧乳以下
		詳細文文字列受け渡し変数 += @"%TEXTR("작은 가슴/작은 가슴")%에%TEXTR("페니스/자지/고추")%을(를) 비벼댄다"
	ELSE
		詳細文文字列受け渡し変数 += @"%TEXTR("가슴/유방/가슴")%으로%TEXTR("페니스/자지/고추")%을(를) 끼워졌다"
	ENDIF
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_指
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_強占有
	CASE 部位_口
		IF プレイヤー番号 < 0 || ターゲット番号 < 0
			;キャラ不明の場合は一般論
			RETURN 部位状況_届かない
		ELSEIF TALENT:ターゲット番号:体格 == 体格_ハーヴィン並 && TALENT:プレイヤー番号:体格 >= 体格_普通
			;ターゲット側がハーヴィン並体格でプレイヤーがそれなりの体格なら抱え込みパイズリとなり届く
			RETURN 部位状況_なし
		ELSE
			RETURN 部位状況_届かない
		ENDIF
	CASE 部位_胸
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_強占有
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


