@MODETYPE_フェラチオ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

詳細文文字列受け渡し変数 = 
IF 処理モード == "写真用文字列_プレイヤー"
	;人妻の場合
	SIF TALENT:ターゲット番号:恋人持ち == 2
		詳細文文字列受け渡し変数 = 남편을 배신하고
	;恋人あり
	SIF TALENT:ターゲット番号:恋人持ち == 1
		詳細文文字列受け渡し変数 = 연인을 배신하고
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 += @"%CALLNAME:ターゲット番号%의%TEXTR("자지/고추/페니스")%%TEXTR("을 구강성교하다/에 혀를 훑다/를 핥아 빨다/에 공손히 키스를 바치다/에 입술을 바쳐 봉사하다/을 입술로 애무하다/을 혀로 애무하다/에 자지 키스 봉사하다")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:3
	CASE 0
		詳細文文字列受け渡し変数 = %TEXTR("자지/페니스")%을(를)%CALLNAME:プレイヤー番号%에게%TEXTR("구강성교당하고 있다/핥아 빨리고 있다/입술과 혀로 애무당하고 있다/구순애무당하고 있다")%
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%의%TEXTR("입술/키스")%을(를)%TEXTR("자지/고추/페니스")%에 바쳐지고 있다
	CASE 2
		詳細文文字列受け渡し変数 = %TEXTR("자지/페니스")%에%CALLNAME:プレイヤー番号%의%TEXTR("구순 봉사/펠라치오 봉사/빨기 봉사")%을(를)%TEXTR("바쳐지고/받고")% 있다
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		;プレイヤーの胸には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


@MODETYPE_パイズリフェラ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード
詳細文文字列受け渡し変数 = 
IF 処理モード == "写真用文字列_プレイヤー"
	;人妻の場合
	SIF TALENT:ターゲット番号:恋人持ち == 2
		詳細文文字列受け渡し変数 = 남편을 배신하고
	;恋人あり
	SIF TALENT:ターゲット番号:恋人持ち == 1
		詳細文文字列受け渡し変数 = 연인을 배신하고
	
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 += @"%CALLNAME:ターゲット番号%의%TEXTR("자지/고추/페니스")%을(를)"
	
		IF TALENT:ターゲット番号:バストサイズ == 3
		;魔乳
			詳細文文字列受け渡し変数 += @"묵직하게 무거운 마유로 끼워"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 2
		;爆乳
			詳細文文字列受け渡し変数 += @"탐스럽게 영근 폭유로 끼워"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 1
		;巨乳
			詳細文文字列受け渡し変数 += @"풍만하게 자란 거유로 끼워"
		ELSEIF TALENT:ターゲット番号:バストサイズ == 0
		;普乳
			詳細文文字列受け渡し変数 += @"부드러운%TEXTR("가슴/미유")%으로 끼워"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -1
		;貧乳
			詳細文文字列受け渡し変数 += @"톡 쏘는 탄력 있는%TEXTR("작은 가슴/미유")%으로%TEXTR("대접하고/자극하고")%"
		ELSEIF TALENT:ターゲット番号:バストサイズ == -2
		;絶壁
			詳細文文字列受け渡し変数 += @"자그마한 미유로%TEXTR("대접하고/자극하고")%"
		ENDIF
	詳細文文字列受け渡し変数 += @"、귀두%TEXTR("를 빨아 애무한다/를 물고 빤다/를 혀끝으로 애무한다/에 입맞춘다")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	SELECTCASE RAND:3
	CASE 0
		詳細文文字列受け渡し変数 = %TEXTR("자지/페니스")%를%CALLNAME:プレイヤー番号%에게 가슴으로 애무당하고 있다
	CASE 1
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%의 %TEXTR("가슴/젖가슴")%과 %TEXTR("입술/입")%으로 %TEXTR("자지/고추/페니스")%를 애무당하고 있다
	CASE 2
		詳細文文字列受け渡し変数 = %CALLNAME:プレイヤー番号%의 %TEXTR("가슴/젖가슴")%과 %TEXTR("입술/입")%에게 %TEXTR("자지/고추/페니스")% 봉사를 받고 있다
	ENDSELECT
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;キスは両者口のみ専有
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_指
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_強占有
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


@MODETYPE_イラマチオ(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

詳細文文字列受け渡し変数 = 
IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = %CALLNAME:ターゲット番号%
	;角掴みになっている時
	IF TALENT:ターゲット番号:角あり
		詳細文文字列受け渡し変数 += "에게 뿔을 잡혀"
	ELSE
		詳細文文字列受け渡し変数 += "의"
	ENDIF
	詳細文文字列受け渡し変数 += @"%TEXTR("페니스/자지")%를 목구멍 깊숙이까지 %TEXTR("삼켰다/박혔다")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	;角掴みになっている時
	SIF TALENT:ターゲット番号:角あり
		詳細文文字列受け渡し変数 = 뿔을 잡고
	詳細文文字列受け渡し変数 += @"%CALLNAME:プレイヤー番号%의 목구멍 깊숙이까지 %TEXTR("페니스/자지")%를 %TEXTR("밀어 넣었다/쑤셔 넣었다/들이밀었다")%"
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_クリトリス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_口
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_届かない
	CASE 部位_胸
		;プレイヤーの胸には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_膣
		;プレイヤーの膣には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻には届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT



