@MODETYPE_側位(部位番号, 処理モード, プレイヤー番号, ターゲット番号)
#DIM 部位番号
#DIM プレイヤー番号
#DIM ターゲット番号
#DIMS 処理モード

IF 処理モード == "写真用文字列_プレイヤー"
	;モード固有の「～～をする」の文章を入れ込む
	詳細文文字列受け渡し変数 = 옆으로 누운%CALLNAME:ターゲット番号%의 다리를 안고, 페니스를 찔러 넣는다
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ELSEIF 処理モード == "写真用文字列_ターゲット"
	;モード固有の「～～をする」の文章を入れ込む
	IF 陥落チェック_性的(ターゲット番号) == 0 && BASE:酩酊 > 300
		;未陥落の泥酔姦。ヤってる最中にも酩酊は下がり続けるので、泥酔の500からちょい下げて300までで
		;むしろ未陥落でもないと別に酔ってコトに及んだ情報残してもシコくなさそうなので未陥落のみ
		詳細文文字列受け渡し変数 = %TEXTR("취한 기세로/취기에 들떠/만취하여")%,%CALLNAME:プレイヤー番号%
		IF TALENT:ターゲット番号:恋人持ち > 0
			IF RISKY_DAY(ターゲット番号) >= 1
				詳細文文字列受け渡し変数 += @"와 측위로 바람%TEXTR("탁란/임신시켜")%%TEXTR("섹스/야한 짓을")%하고 있다"
			ELSE
				詳細文文字列受け渡し変数 += @"와 측위로 바람%TEXTR("섹스/야한 짓")%하고 있다"
			ENDIF
		ELSE
			詳細文文字列受け渡し変数 += @"에게 측위로 보지를%TEXTR("사용당하고 있다/허락해 버렸다/즐겨지고 있다")%"
		ENDIF
	ELSE
		詳細文文字列受け渡し変数 = %TEXTR("옆으로 누운 채 다리를 벌리고/측위로")%,%CALLNAME:プレイヤー番号%
		IF TALENT:ターゲット番号:恋人持ち > 0
			詳細文文字列受け渡し変数 += @"와 바람%TEXTR("섹스/야한 짓")%하고 있다"
		ELSE
			詳細文文字列受け渡し変数 += @"의%TEXTR("페니스/자지")%를 물었다"
		ENDIF
	ENDIF
	;下の部位専有判定と別の値を返す
	RETURN 部位状況_写真用
ENDIF

;0=ペニス 1=クリ 2=指（手） 3=口 4=胸 5=膣 6=アナル
;挿入で使用するのはペニスと膣で1を返す
SELECTCASE 部位番号
	CASE 部位_ペニス
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_占有
	CASE 部位_口
		;口は届かない
		RETURN 部位状況_届かない
	CASE 部位_胸
		;胸は届かない
		IF ターゲット番号 < 0
			;キャラ不明の場合は一般論
			RETURN 部位状況_届かない
		ELSEIF 処理モード == "ターゲット" && 体格差計算(プレイヤー番号, ターゲット番号) <= -4
			;受け側が大きく体格で劣れば届く
			RETURN 部位状況_なし
		ELSE
			RETURN 部位状況_届かない
		ENDIF
	CASE 部位_膣
		SIF 処理モード == "ターゲット"
			RETURN 部位状況_占有
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
	CASE 部位_アナル
		;プレイヤーの尻は届かない
		SIF 処理モード == "プレイヤー"
			RETURN 部位状況_届かない
ENDSELECT


