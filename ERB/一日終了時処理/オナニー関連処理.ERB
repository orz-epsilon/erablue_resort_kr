;-------------------------------------------------
;ARG番のキャラが野外オナニーを実行するかを返す関数
;実行する場合フラグ立てまで行う
;開始時, 実行中には1を返す
;絶頂時(=終了時), 開始しない時, 途中終了時には0を返す
;-------------------------------------------------
@野外オナニー判定(ARG)
;既に実行中の場合は継続するか判定する
IF TCVAR:ARG:野外オナニー_残時間 > 0 || TCVAR:ARG:野外オナニー_絶頂
	CALL 野外オナニー継続判定(ARG)
	RETURN RESULT
ENDIF
;睡眠中に無意識に……みたいなのも面白そうだけど現在はなし
SIF CFLAG:ARG:睡眠
	RETURN 0
;ダウン時ダメ
SIF BASE:ARG:体力 < 1
	RETURN 0
;性知識が無い場合はオナニー出来ない
SIF 知識素質:ARG:性知識 < 0 && EXP:ARG:自慰経験 <= 0
	RETURN 0
;性感帯無い場合も駄目
SIF !HAS_EROGENOUS_ZONE(ARG)
	RETURN 0
;同行中はやんないよ
SIF CFLAG:ARG:同行
	RETURN 0
SIF CFLAG:ARG:一緒に遊ぶ
	RETURN 0
SIF CFLAG:ARG:デート
	RETURN 0
SIF CFLAG:ARG:うふふ
	RETURN 0
SIF MODE_存在判定_いずれか側("同行系", ARG)
	RETURN 0
SIF MODE_存在判定_いずれか側("キス系", ARG)
	RETURN 0
SIF MODE_存在判定_ターゲット側("全裸散歩", ARG)
	RETURN 0
SIF MODE_存在判定_いずれか側("ハーヴィンオナホ系", ARG)
	RETURN 0
;前回の野外オナニーの正常終了から時間経過してないとしない
;興味の場合は一日一回、愛好の場合は４時間置き（一日２回絶頂まで）、中毒の場合は２時間置き（一日３回絶頂まで）
SELECTCASE 性癖素質:ARG:野外オナニー
	CASE 1
		SIF TCVAR:ARG:野外オナニー_開始時刻
			RETURN 0
	CASE 2
		SIF TCVAR:ARG:野外オナニー_開始時刻 + 4 * 60 > NOW()
			RETURN 0
		SIF TCVAR:ARG:野外オナニー_実行回数 > 1
			RETURN 0
	CASE 3
		SIF TCVAR:ARG:野外オナニー_開始時刻 + 2 * 60 > NOW()
			RETURN 0
		SIF TCVAR:ARG:野外オナニー_実行回数 > 2
			RETURN 0
ENDSELECT
;性欲や自慰しやすいなどの条件を見る
IF BASE:ARG:性欲 >= (1000 - TALENT:ARG:自慰しやすい * 200)
	;通常オナニー条件
ELSEIF CFLAG:ARG:オナニー覚えたて
	;覚えたて補正
ELSEIF 性癖素質:ARG:野外オナニー == 3
	;中毒時は性欲関係なし
ELSE
	;上記の条件に当てはまらないなら0で戻す
	RETURN 0
ENDIF
;既に誰かが同じ場所で隠密してるなら出来ない
FOR LOCAL, 0, CHARANUM
	SIF LOCAL == ARG
		CONTINUE
	SIF IS_SAME_ROOM(ARG, LOCAL) && CFLAG:LOCAL:隠密
		RETURN 0
NEXT

;実行処理
TCVAR:ARG:野外オナニー_開始時刻 = NOW()
TCVAR:ARG:野外オナニー_残時間 = 15
TCVAR:ARG:野外オナニー_部位 = WEIGHTED_RANDOM_PART(ARG)
CFLAG:ARG:隠密 = 1
RETURN 1

;-------------------------------------------------
;ARG番のキャラが野外オナニーを継続するか返す関数
;継続する場合には1を返す
;被発見や絶頂時などで終了する場合には0を返す
;-------------------------------------------------
@野外オナニー継続判定(ARG)
#DIM 同行者未陥落
#DIM 同行者フラグ

;時間経過させる前に残時間が0になってる場合は異常事態なので途中終了させる
IF TCVAR:ARG:野外オナニー_残時間 <= 0 && TCVAR:ARG:野外オナニー_絶頂 == 0
	CALL 野外オナニー途中終了(ARG)
	CFLAG:ARG:隠密 = 0
	RETURN 0
ENDIF
;オナニー中にあなたが物陰に入ってきたパターン
IF CFLAG:PLAYER:隠密 && IS_SAME_ROOM(ARG)
	同行者未陥落 = 0
	同行者フラグ = 0
	FOR LOCAL, 1, CHARANUM
		SIF TARGET:LOCAL < 1
			BREAK
		IF CFLAG:(TARGET:LOCAL):同行
			同行者フラグ = 1
			SIF !陥落チェック_性的(TARGET:LOCAL)
				同行者未陥落 = 1
		ENDIF
	NEXT
	IF 陥落チェック_性的(ARG) && 同行者未陥落 == 0
		PRINTFORML %CALLNAME:PLAYER%이(가) 물그림자에 들어서자, 그곳에 있던%CALLNAME:ARG%은(는) 몹시 당황한 모습을 보였지만
		PRINTFORMW 나타난 것이%CALLNAME:PLAYER%인 것을 알자, 안심한 듯하다
		PRINTL
		PRINTFORML %CALLNAME:ARG%의 복장은 흐트러져 있었고, 뺨을 붉힌 모습은 매우 선정적이다……
		IF 同行者フラグ
			PRINTFORML %CALLNAME:ARG%들을 끌어안겠습니까?
		ELSE
			PRINTFORML %CALLNAME:ARG%을(를) 끌어안겠습니까?
		ENDIF
		PRINTBUTTONLC "[0] 예", 0
		PRINTBUTTONLC "[1] 아니요", 1
		PRINTL
		BINPUT
		IF RESULT == 0
			PRINTFORMW %CALLNAME:PLAYER%은(는) 그대로 물그림자에서%CALLNAME:ARG%을(를) 끌어안았다……。
			CALL 野外オナニー途中終了(ARG)
			CFLAG:PLAYER:うふふ = 1
			CFLAG:ARG:隠密 = 1
			PALAM:ARG:欲情 = MAX(30000, (PALAM:ARG:欲情 - 10000) * 3 / 2) ;見つかったけどPLAYERだったので安心
			PALAM:ARG:潤滑 = MAX(30000, PALAM:ARG:潤滑 * 11 / 10)
			CALL うふふ開始キャラ処理(ARG, "キャラの野外オナニー目撃")
			FOR LOCAL, 1, CHARANUM
				SIF TARGET:LOCAL < 1
					BREAK
				IF CFLAG:(TARGET:LOCAL):同行
					CFLAG:(TARGET:LOCAL):隠密 = 1
					CALL うふふ開始キャラ処理(TARGET:LOCAL, @"キャラの野外オナニー目撃_同行_{ARG}")
				ENDIF
			NEXT
			;処理の誤爆を防ぐためオートコマンド周りリセット
			TFLAG:オートコマンドフラグ = 0
			AUTOCOM_実行番号保存 = -1
			AUTOCOM_対象番号保存 = -1
			自慰ターゲット保存 = -1
			RETURN 0
		ELSE
			PRINTFORML 즐거움을 방해해서는 미안하다.
			PRINTFORMW %CALLNAME:PLAYER%은(는)%CALLNAME:ARG%에게 가볍게 사과하고, 그 자리를 떠났다.
			CFLAG:PLAYER:隠密 = 0
			FOR LOCAL, 1, CHARANUM
				SIF TARGET:LOCAL < 1
					BREAK
				SIF TARGET:LOCAL == ARG
					CONTINUE
				IF CFLAG:(TARGET:LOCAL):同行
					CFLAG:(TARGET:LOCAL):隠密 = 0
				ENDIF
			NEXT
			;下の処理へ進む
		ENDIF
	ELSE
		DRAWLINE
		PRINTFORML %CALLNAME:PLAYER%이(가) 물그림자에 들어서자, 그곳에 있던%CALLNAME:ARG%은(는) 몹시 당황한 모습으로 몸단장을 하고
		PRINTFORMW %CALLNAME:PLAYER%와 눈을 마주치지 않고 물그림자에서 떠나갔다…
		CALL 野外オナニー途中終了(ARG)
		CFLAG:ARG:隠密 = 0
		RETURN 0
	ENDIF
ENDIF

;経過時間で継続か絶頂か判定
;残時間が0になると絶頂して正常終了
;終了処理自体は@野外オナニー実処理で行う
;どちらでもこの時点では隠密を継続
TCVAR:ARG:野外オナニー_残時間 = MAX(0, TCVAR:ARG:野外オナニー_残時間 - TIME_PROGRESS(TFLAG:行動前時刻))
CFLAG:ARG:隠密 = 1
IF TCVAR:ARG:野外オナニー_残時間 > 0
	RETURN 1
ELSEIF TCVAR:ARG:野外オナニー_絶頂
	IF TCVAR:ARG:野外オナニー_絶頂 - TIME_PROGRESS(TFLAG:行動前時刻) < 1
		CALL 野外オナニー途中終了(ARG)
		CFLAG:ARG:隠密 = 0
		RETURN 0
	ENDIF
ELSE
	TCVAR:ARG:野外オナニー_絶頂 = 1
	TCVAR:ARG:野外オナニー_残時間 = 0
	RETURN 0
ENDIF

