IF 一日終了時オナニーOPTION:(TALENT:ARG:性別) == 2
	IF BASE:ARG:性欲 >= (1000 - TALENT:ARG:自慰しやすい * 200)
		BASE:ARG:性欲 = MAX(BASE:ARG:性欲 - 600, 0)
	ENDIF
	RETURN 0
ENDIF
